{
  "hosting": {
    "public": "public",
    "rewrites" :[
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers" : [
      {
        "source": "**/*.@(js.map|js|css|txt|html|png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "s-maxage=31536000,immutable"
          }
        ]
      }
    ]
  }
}
